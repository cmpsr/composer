{
  "name": "composer",
  "version": "0.0.1",
  "main": "index.js",
  "author": "Steve Cox <stevejcox@me.com>",
  "license": "MIT",
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build --stream",
    "bbuild": "lerna run libbuild --stream",
    "build-storybook": "lerna run libbuild --stream --scope=@cmpsr/components && lerna run build-storybook --stream",
    "test": "lerna run test --stream",
    "test:watch": "lerna run --parallel test:watch --stream",
    "test:coverage": "lerna run --parallel test:coverage --stream",
    "lint": "eslint . --ext .tsx,.ts,.js",
    "lint-and-fix": "eslint . --ext .tsx,.ts,.js --fix",
    "publish-ci": "lerna run libbuild --stream && lerna publish from-package -y --no-verify-access",
    "prerelease": "yarn build",
    "release": "lerna publish"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "resolutions": {
    "babel-core": "^7.0.0-bridge.0"
  },
  "dependencies": {
    "@changesets/cli": "^2.9.1",
    "lerna": "^3.20.2"
  },
  "devDependencies": {
