{
  "name": "@cmpsr/blocks",
  "repository": {
    "url": "ssh://git@github.com:cmpsr/composer.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "version": "0.0.1",
  "author": "Takeichi Kanzaki Cabrera <tkanzakic@hey.com>",
  "license": "MIT",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "/lib",
    "package.json"
  ],
  "private": true,
  "scripts": {
    "libbuild": "rimraf lib && npx swc ./src -d lib -s --ignore '**/*.test.ts,**/*.stories.ts' && find lib -type f \\( -name '*.test.*' -o -name '*.stories.*' \\) -exec rm {} +",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type:check": "yarn tsc --project tsconfig.json --noEmit",
    "type:emit": "tsc --project ./tsconfig.json -d --declarationDir lib --declarationMap --emitDeclarationOnly && find lib -type f \\( -name '*.test.*' -o -name '*.stories.*' \\) -exec rm {} +"
  },
  "peerDependencies": {
    "react": "^18.1.0",
    "@cmpsr/components": "2.0.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.150",
    "@swc/jest": "^0.2.20",
    "@testing-library/jest-dom": "^5.16.2",
    "@types/jest": "^27.0.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "swc-loader": "^0.1.15"
  },
  "engines": {
    "node": ">=14 <17"
  }
}
