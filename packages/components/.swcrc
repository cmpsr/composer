{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": false,
      "dynamicImport": false
    },
    "paths": {
      "components": ["./src/components"],
      "theme": ["./src/theme"],
      "hooks": ["./src/hooks"]
    },
    "baseUrl": "."
  },
  "module": {
    "type": "commonjs",
    "strict": false,
    "strictMode": true,
    "lazy": false,
    "noInterop": false
  }
}
