{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": false,
      "dynamicImport": false
    },
    "paths": {
      "@components": ["./src/components"],
      "@hooks": ["./src/hooks"],
      "@tests/*": ["./src/tests/*"],
      "@theme": ["./src/theme"]
    },
    "minify": {
      "compress": {
        "keep_classnames": true,
        "sequences": false
      },
      "mangle": false
    },
    "keepClassNames": true,
    "baseUrl": "."
  },
  "module": {
    "type": "commonjs",
    "strict": false,
    "strictMode": true,
    "lazy": false,
    "noInterop": false
  },
  "env": {
    "targets": "> 0.25%, not dead"
  }
}
