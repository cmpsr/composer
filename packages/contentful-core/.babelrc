{
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    "import-graphql",
    [
      "module-resolver",
      {
        "alias": {
          "client": "./src/client",
          "components": "./src/components",
          "constants": "./src/constants",
          "context": "./src/context",
          "renderers": "./src/renderers",
          "scripts": "./src/scripts"
        }
      }
    ]
  ],
  "presets": [
    "@babel/preset-typescript",
    "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ]
}
