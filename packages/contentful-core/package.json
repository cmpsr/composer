{
  "name": "@cmpsr/contentful-core",
  "version": "0.1.3",
  "license": "MIT",
  "description": "Base Composer components for standing up a React/NextJS app that can dynamically render components based on Contentful Model types mapped to components and queries.",
  "author": "Ryan Hefner <hi@ryanhefner.com>",
  "repository": "cmpsr/composer",
  "homepage": "https://github.com/cmpsr/composer/packages/contentful-core#readme",
  "bugs": {
    "url": "https://github.com/cmpsr/composer/issues"
  },
  "main": "index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.js",
  "directories": {
    "lib": "lib",
    "scripts": "scripts",
    "test": "__tests__"
  },
  "files": [],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "yarn clean",
    "build": "mkdir dist && cp -R ./scripts ./dist/scripts && node ../../scripts/build",
    "test": "jest --env=jest-environment-jsdom-sixteen",
    "test:watch": "jest --watch --env=jest-environment-jsdom-sixteen",
    "test:coverage": "jest --coverage --env=jest-environment-jsdom-sixteen",
    "graphql:schema": "node ./scripts/graphql-schema.js",
    "graphql:types": "node ./scripts/graphql-types.js"
  },
  "peerDependencies": {
    "react": ">=16.3",
    "react-dom": ">=16.3"
  },
  "dependencies": {
    "@types/invariant": "^2.2.33",
    "apollo-link-http": "^1.5.17",
    "dotenv": "^8.2.0",
    "invariant": "^2.2.4"
  },
  "devDependencies": {}
}
