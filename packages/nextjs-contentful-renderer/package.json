    "remark-gfm": "^3.0.1"
  },
  "devDependencies": {
    "@apollo/client": "^3.6.2",
    "@cmpsr/blocks": "^14.1.11",
    "@cmpsr/carousel": "^4.0.30",
    "@cmpsr/cml": "^0.10.2",
    "@cmpsr/components": "^7.17.0",
    "@cmpsr/signalwire": "^2.0.29",
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.150",
    "@swc/jest": "^0.2.20",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/cookie": "^0.5.1",
    "@types/jest": "^27.5.0",
    "jest": "^28.1.0",
    "next": "^12.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.6.4"
  },
