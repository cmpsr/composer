{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": true,
      "dynamicImport": false
    },
    "minify": {
      "compress": {
        "keep_classnames": true,
        "sequences": false
      },
      "mangle": false
    },
    "baseUrl": "."
  },
  "module": {
    "type": "commonjs",
    "strict": false,
    "strictMode": true,
    "lazy": false,
    "noInterop": false
  },
  "env": {
    "targets": "> 0.25%, not dead"
  }
}
