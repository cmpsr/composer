{
  "name": "@cmpsr/signalwire",
  "repository": {
    "url": "ssh://git@github.com:cmpsr/composer.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "version": "2.0.29",
